<body>
    <header>

        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light fixed-top mb-2 p-3" style=" background-color: lightgray;">
            <div class="container-fluid">
                <a class="navbar-brand font-2 font-weight-bold" asp-area="" asp-controller="Base" asp-action="Index">TeeBlog</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse justify-content-end">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link text-dark font" asp-controller="Base" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark font" asp-controller="Category" asp-action="Category"> Category </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark font" asp-controller="Video" asp-action="Video">Videos</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark font" asp-controller="Base" asp-action="About">About</a>
                        </li> 
                        <li class="nav-item">
                            <a class="nav-link text-dark font" asp-controller="FAQs" asp-action="FAQs">FAQs</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark font" asp-controller="Contact" asp-action="Contact">Contact</a>
                        </li>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <li class="nav-item">
                                <a class="nav-link text-white font btn btn-success m-2 p-2" asp-action="Dashboard" asp-controller="Admin">Dashboard</a>
                            </li>
                        }
                        else
                        {
                            <li class="nav-item">
                                <a class="nav-link text-white font btn btn-success m-2 p-2" asp-action="Login" asp-controller="Account">Login</a>
                            </li>
                        }
                        @if (!User.Identity.IsAuthenticated)
                        {
                            <li class="nav-item">
                                <a class="nav-link text-white font btn btn-success m-2 p-2" asp-action="Register" asp-controller="Account">Register</a>
                            </li>
                        }
                        
                    </ul>
                </div>
            </div>
        </nav>
    </header>

