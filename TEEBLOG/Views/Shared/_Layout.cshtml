    <script>
        function newSuccessAlert(message, url) {
            debugger;
            Swal.fire({
                title: "SUCCEEDED",
                icon: 'success',
                text: message,
                confirmButtonColor: 'blue',
            }).then((isConfirm) => {
                if (isConfirm) {
                    location.href = url;
                }
            })

        }

        function errorAlert(message) {
            Swal.fire({
                icon: 'error',
                text: message,
                timer: 60000,
                confirmButtonColor: 'green',
            })
        }

        function infoAlert(message) {
            Swal.fire({
                icon: 'info',
                text: message,
                confirmButtonColor: 'green',
            })
        }

