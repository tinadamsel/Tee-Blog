    <script>
        function errorAlert(message) {
            Swal.fire({
                title: 'Oops',
                icon: 'error',
                text: message,
                timer: 60000,
                confirmButtonColor: "red",
            })
        }

        function infoAlert(message) {
            Swal.fire({
                title: 'Incorrect Details',
                icon: 'warning',
                text: message,
                confirmButtonColor: "skyblue",
            })
        }

        function infoAlertWithStyles(message) {
            Swal.fire({
                title: 'Incomplete Payment',
                icon: 'info',
                text: message,
                confirmButtonColor: "#FF9B44",
            })
        }

        function successAlert(message) {
            Swal.fire({
                icon: 'success',
                text: message,
                timer: 60000,
                confirmButtonColor: "#FF9B44",
            })
        }

        function newErrorAlert(message, url) {
            Swal.fire({
                icon: 'error',
                text: message,
                confirmButtonColor: "#FF9B44",
            }).then((isConfirm) => {
                if (isConfirm) {
                    location.reload(url);
                }
            })
        }

        function newSuccessAlert(message, url) {
            Swal.fire({
                title: 'Successful',
                icon: 'success',
                text: message,
                confirmButtonColor: "green",
            }).then((isConfirm) => {
                if (isConfirm) {
                    location.href = url;
                }
            })

        }
        function successAlertWithRedirect(message, url) {
            Swal.fire({
                title: "Success",
                text: message,
                icon: "success",
                timer: 30000,
                overlay: "background - color: rgba(43, 165, 137, 0.45)",
                confirmButtonColor: "#FF9B44",
            }).then(function () {

                location.href = url;
            })
        }
    </script>


