                <a asp-controller="Admin" asp-action="AddBlogPost"> <i class="fa fa-plus"></i>&nbsp; AddBlogPost </a>
            </button>
        </div>
    </div>

    <div class="container-fluid">
        <table class="table table-bordered table-striped">
            <thead>
            <th> Names</th>
            <th> DateCreated</th>
            <th> Heading</th>
            <th> Short Description</th>
            </thead>
        </table>
